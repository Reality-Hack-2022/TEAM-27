using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.UI;

public class VFXSizeControl : MonoBehaviour
{
	
	
	private Slider vfx_size;
	
	public float size;
	
	
	
    void Start()
    {
        vfx_size = this.gameObject.GetComponent<Slider>();
		
    }

    
    public void SetSize()
    {
        size = vfx_size.value;
		
		
    }
}
